{% extends "base" %}

{% block content %}

{{ super() }}

<div class="container">

	<div class="row">

		<div class="col-sm m-4">

			<h2>Exec Positions</h2>

			<div class="list-group">

				{% for position in exec_positions %}

				<a href="/elections/{{ position.id }}" class="list-group-item list-group-item-action">
					{{ position.title }}
				</a>

				{% endfor %}

			</div>

		</div>

	</div>

</div>

{% endblock content %}
